<dialect xmlns="http://www.thymeleaf.org/extras/dialect"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.thymeleaf.org/extras/dialect
                             http://www.thymeleaf.org/xsd/thymeleaf-extras-dialect-2.1.xsd"
         prefix="blc"
         namespace-uri="http://www.broadleafcommerce.com/"
         class="com.wakacommerce.common.web.dialect.BLCDialect">
    
    <!-- Element processors -->
    <element-processor name="categories" class="com.wakacommerce.core.web.processor.CategoriesProcessor" />
    <element-processor name="form" class="com.wakacommerce.core.web.processor.FormProcessor" />
    <element-processor name="googleanalytics" class="com.wakacommerce.core.web.processor.GoogleAnalyticsProcessor" />
    <element-processor name="google_universal_analytics" class="com.wakacommerce.core.web.processor.GoogleUniversalAnalyticsProcessor" />
    <element-processor name="head" class="com.wakacommerce.core.web.processor.HeadProcessor" />
    <element-processor name="named_order" class="com.wakacommerce.core.web.processor.NamedOrderProcessor" />
    <element-processor name="one_page_checkout" class="com.wakacommerce.core.web.processor.OnePageCheckoutProcessor" />
    <element-processor name="product_option_display" class="com.wakacommerce.core.web.processor.ProductOptionDisplayProcessor" />
    <element-processor name="product_options" class="com.wakacommerce.core.web.processor.ProductOptionsProcessor" />
    <element-processor name="ratings" class="com.wakacommerce.core.web.processor.RatingsProcessor" />
    <element-processor name="related_products" class="com.wakacommerce.core.web.processor.RelatedProductProcessor" />
    <element-processor name="uncacheabledata" class="com.wakacommerce.core.web.processor.UncacheableDataProcessor" />
    
    <!-- Attribute processors -->
    <attribute-processor name="addsortlink" class="com.wakacommerce.core.web.processor.AddSortLinkProcessor" />
    <attribute-processor name="cache" class="com.wakacommerce.core.web.processor.BroadleafCacheProcessor" />
    <attribute-processor name="addsortlink" class="com.wakacommerce.core.web.processor.AddSortLinkProcessor" />
    <attribute-processor name="paginationpagelink" class="com.wakacommerce.core.web.processor.PaginationPageLinkProcessor" />
    <attribute-processor name="pagination-size-link" class="com.wakacommerce.core.web.processor.PaginationSizeLinkProcessor" />
    <attribute-processor name="pagination-sort-link" class="com.wakacommerce.core.web.processor.PaginationSortLinkProcessor" />
    <attribute-processor name="price" class="com.wakacommerce.core.web.processor.PriceTextDisplayProcessor" />
    <attribute-processor name="togglefacetlink" class="com.wakacommerce.core.web.processor.ToggleFacetLinkProcessor" />
    <attribute-processor name="removefacetvalueslink" class="com.wakacommerce.core.web.processor.RemoveFacetValuesLinkProcessor" />
    
</dialect>