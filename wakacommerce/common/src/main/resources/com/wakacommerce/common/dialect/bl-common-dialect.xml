<?xml version="1.0" encoding="UTF-8"?>
<dialect xmlns="http://www.thymeleaf.org/extras/dialect"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.thymeleaf.org/extras/dialect
                             http://www.thymeleaf.org/xsd/thymeleaf-extras-dialect-2.1.xsd"
         prefix="blc"
         namespace-uri="http://www.broadleafcommerce.com"
         class="com.wakacommerce.common.web.dialect.BLCDialect">

    <element-processor name="config" class="com.wakacommerce.common.web.processor.ConfigVariableProcessor" />
    <element-processor name="enumeration" class="com.wakacommerce.common.web.processor.DataDrivenEnumerationProcessor" />
    <element-processor name="bundle" class="com.wakacommerce.common.web.processor.ResourceBundleProcessor" />
    
    <expression-object name="props" class="com.wakacommerce.common.web.expression.PropertiesVariableExpression" />
    <expression-object name="brc" class="com.wakacommerce.common.web.expression.BRCVariableExpression" />
    
</dialect>